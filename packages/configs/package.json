{
  "name": "@oliviacl/tutorial-frontend-config",
  "version": "1.0.0",
  "description": "Shareable ESLint and Prettier configurations for frontend projects",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./eslint": {
      "types": "./eslint.d.ts",
      "default": "./eslint.js"
    },
    "./prettier": {
      "types": "./prettier.d.ts",
      "default": "./prettier.js"
    }
  },
  "keywords": ["eslint", "prettier", "config", "javascript", "typescript", "frontend"],
  "author": "oliviacl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liv7c/tutorial-frontend-config.git",
    "directory": "packages/configs"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.0.0"
  },
  "dependencies": {
    "@eslint/js": "^9.39.1",
    "typescript-eslint": "^8.46.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1"
  },
  "devDependencies": {
    "@types/eslint": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
